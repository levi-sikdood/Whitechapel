<script>document.addEventListener("DOMContentLoaded", function () {
    let parentNavItem = document.getElementById("nav-dropdown"); // The nav item with the dropdown
    let parentLink = document.getElementById("parent-link"); // The parent link
    if (parentNavItem && parentLink) {
        let hasDropdown = parentNavItem.querySelector("#dropdown-menu"); // Check if dropdown exists
        if (hasDropdown) {
            parentLink.removeAttribute("href"); // Disable the parent link
            parentLink.style.cursor = "default"; // Change cursor to indicate it's not clickable
        }
    }
});
